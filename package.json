{
  "name": "alex_dalene_exam2",
  "version": "0.0.1",
  "description": "Website for an Auction House",
  "main": "src/index.js",
  "author": "Alex Dalene <aledal51469@stud.noroff.no>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "cypress": "cypress open",
    "test": "jest src",
    "start": "live-server",
    "build": "postcss ./src/css/input.css -o ./public/css/output.css",
    "watch": "postcss --watch ./src/css/input.css -o ./public/css/output.css & yarn start"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "cypress": "9.6.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.2.0",
    "live-server": "1.2.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "2.6.2",
    "tailwindcss": "^3.3.5"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier",
      "cypress",
      "jest"
    ],
    "env": {
      "cypress/globals": true,
      "jest/globals": true
    },
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {}
}
